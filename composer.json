{
    "name": "ngabor84/booking-api",
    "description": "Laravel Lumen based REST-API for booking",
    "keywords": ["api", "booking", "laravel", "lumen"],
    "license": "MIT",
    "type": "project",
    "authors": [
        {
            "name": "Gábor Németh",
            "email": "negabor@gmail.com",
            "homepage": "https://nemeth-gabor.hu",
            "role": "developer"
        }
    ],
    "require": {
        "php": ">=7.1",
        "laravel/lumen-framework": "~5.4",
        "vlucas/phpdotenv": "~2.2",
        "dingo/api": "1.0.*@dev",
        "tymon/jwt-auth": "~1.0",
        "guzzlehttp/guzzle": "~6.0",
        "lukasoppermann/http-status": "^2.0",
        "jakubkratina/lumen-dingo-route-list": "^0.9.5",
        "ngabor84/lumen-api-query-parser": "~1.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "~0.9",
        "phpunit/phpunit": "~6.1",
        "squizlabs/php_codesniffer": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Test\\": "tests"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/",
            "database/"
        ]
    },
    "scripts": {
        "check": [
            "@cs-check",
            "@test"
        ],
        "cs-check": "./vendor/bin/phpcs --colors",
        "test": "./vendor/bin/phpunit --colors=always"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
